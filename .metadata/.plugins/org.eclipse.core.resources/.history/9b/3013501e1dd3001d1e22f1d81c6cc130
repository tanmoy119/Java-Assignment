package amazone;

public class Students {

	private String name;
	private String section;
	private int rollNum;
	private int Class;
	
	
	//Constructor 
//	public Students(String name, String section, int rollNum) {
//		this.name=name;
//		this.section=section;
//		this.rollNum=rollNum;
//		
//	}
	public Students(String name, String section, int rollNum, int Class) {
		this.name=name;
		this.section=section;
		this.rollNum=rollNum;
		this.Class=Class;
		
	}
	
	public String getName() {
		return this.name ;
	}
	public String getSection() {
		return this.section ;
	}
	public int getroll() {
		
		return this.rollNum ;
	}
	public int getClass() {
		
		return this.Class ;
	}
	
	
	public void setName(String name) {
		this.name=name;
	}
	public void setSection(String section) {
		this.section=section;
	}
	public void setRol(int rollNum) {
		this.rollNum=rollNum;
	}
	
	public static void main(String[] args) {
		
	}
	
	
}
